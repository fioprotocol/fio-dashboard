  "main": "index.js",
  "scripts": {
    "preinstall": "npm i github:barrysteyn/node-scrypt#fb60a8d3c158fe115a624b5ffa7480f3a24b03fb",
    "client:start": "cd ./client && react-app-rewired start",
    "client:build": "cd ./client && react-app-rewired --expose-gc --max-old-space-size=3072 build",
    "client:test": "cd ./client && CI=true react-app-rewired test --env=jsdom",
    "client:analyze": "cd ./client && source-map-explorer build/static/js/main.*",
    "server:dev": "npm run migrate && nodemon -L --inspect --es-module-specifier-resolution=node --watch server server/src/index.mjs",
    "widget:as:start": "export REACT_APP_WIDGET=crypto-handle-selection && npm run client:start",
    "widget:as:build": "export REACT_APP_WIDGET=crypto-handle-selection && npm run client:build && bash -c 'source ./bin/build-widget.sh'",
    "lint": "eslint ./server/src ./client/src --ext js,jsx,mjs,ts,tsx",
    "lint:fix": "eslint ./server/src ./client/src --ext js,jsx,mjs,ts,tsx --fix",
    "server:prod": "npm run migrate && node --es-module-specifier-resolution=node server/src/index.mjs",
    "scheduler": "node --es-module-specifier-resolution=node server/src/scheduler.mjs",
    "migrate": "sequelize db:migrate",
    "test:api": "cross-env TEST_MODE=0 LOG_LEVEL=error babel-node --extensions .js,.mjs node_modules/.bin/_mocha --exit --timeout 60000 --reporter spec --ui qunit server/tests/api/*",
    "test": "npm run lint && npm run test:create.db && npm run test:api",
    "test:create.db": "node --experimental-modules server/tests/createDB.mjs"
  },
  "license": "ISC",
