{
  "name": "fio-dapp",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client:start": "cd ./client && react-app-rewired start",
    "client:build": "cd ./client && react-app-rewired --expose-gc --max-old-space-size=8192 build",
    "client:test": "cd ./client && CI=true react-app-rewired test --env=jsdom",
    "client:analyze": "cd ./client && source-map-explorer build/static/js/main.*",
    "server:dev": "npm run migrate && nodemon -L --inspect --experimental-modules --watch server server/src/index.mjs",
    "lint": "eslint ./server/src ./client/src --ext js,jsx,mjs",
    "lint:fix": "eslint ./server/src ./client/src --ext js,jsx,mjs --fix",
    "server:prod": "npm run migrate && node --experimental-modules server/src/index.mjs",
    "migrate": "sequelize db:migrate",
    "test:api": "cross-env TEST_MODE=0 LOG_LEVEL=error babel-node --extensions .js,.mjs node_modules/.bin/_mocha --exit --timeout 60000 --reporter spec --ui qunit server/tests/api/*",
    "test": "npm run lint && npm run test:create.db && npm run test:api",
    "test:create.db": "node --experimental-modules server/tests/createDB.mjs"
  },
  "license": "ISC",
