{
  "name": "@fioprotocol/fio-app",
  "version": "1.32",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm i github:barrysteyn/node-scrypt#fb60a8d3c158fe115a624b5ffa7480f3a24b03fb",
    "postinstall": "node -e \"const fs = require('fs'); const path = require('path'); const { execSync } = require('child_process'); const nodeModulesPath = path.join(__dirname, 'node_modules'); const pkgPath = path.join(nodeModulesPath, 'ledgerjs-hw-app-fio'); const tempPath = path.join(nodeModulesPath, '.temp-ledger-fio'); if (fs.existsSync(path.join(pkgPath, 'package.json'))) { console.log('ledgerjs-hw-app-fio already installed, skipping...'); process.exit(0); } console.log('Installing ledgerjs-hw-app-fio from GitHub...'); if (!fs.existsSync(nodeModulesPath)) { fs.mkdirSync(nodeModulesPath, { recursive: true }); } console.log('Cloning repository...'); execSync('git clone --branch memo_hotfix --depth 1 https://github.com/vacuumlabs/ledger-fio.git \\\"' + tempPath + '\\\"', { stdio: 'inherit' }); console.log('Extracting ledgerjs-fio subdirectory...'); const submodulePath = path.join(tempPath, 'ledgerjs-fio'); if (!fs.existsSync(submodulePath)) { throw new Error('ledgerjs-fio subdirectory not found in repository'); } fs.renameSync(submodulePath, pkgPath); fs.rmSync(tempPath, { recursive: true, force: true }); console.log('Installing dependencies in ledgerjs-hw-app-fio...'); execSync('npm i --legacy-peer-deps', { cwd: pkgPath, stdio: 'inherit' }); console.log('Running prepublish...'); execSync('npm run prepublish', { cwd: pkgPath, stdio: 'inherit' }); console.log('ledgerjs-hw-app-fio installed successfully!');\"",
    "client:start": "cd ./client && react-app-rewired start",
    "client:build": "cd ./client && react-app-rewired --expose-gc --max-old-space-size=8192 build && react-snap",
    "client:build:no-snap": "cd ./client && react-app-rewired --expose-gc --max-old-space-size=8192 build",
    "client:test": "cd ./client && CI=true react-app-rewired test --env=jsdom",
    "client:analyze": "cd ./client && source-map-explorer build/static/js/main.*.js",
    "server:dev": "npm run migrate && nodemon -L --inspect --es-module-specifier-resolution=node --watch server server/src/index.mjs",
    "admin:as:start": "export REACT_APP_API_BASE_URL=http://localhost:3001/ PORT=3001 REACT_APP_IS_ADMIN=true && npm run client:start",
    "admin:as:build": "export REACT_APP_IS_ADMIN=true && npm run client:build:no-snap",
    "lint": "eslint ./server/src ./client/src --ext js,mjs,ts,tsx",
    "lint:fix": "eslint ./server/src ./client/src --ext js,mjs,ts,tsx --fix",
    "server:prod": "npm run migrate && node --es-module-specifier-resolution=node server/src/index.mjs",
    "scheduler": "JOB_LIST=apiUrls,cart,gatedRegistrationTokens,emails,missingBcTxs,domainsExpiration,retryForkedTransactions,fioProxies  node --es-module-specifier-resolution=node server/src/scheduler.mjs",
    "scheduler:wallet-data": "JOB_LIST=walletData node --es-module-specifier-resolution=node server/src/scheduler.mjs",
    "scheduler:wrap-status": "JOB_LIST=wrapStatus node --es-module-specifier-resolution=node server/src/scheduler.mjs",
    "scheduler:orders": "JOB_LIST=orders,txCheck node --es-module-specifier-resolution=node server/src/scheduler.mjs",
    "migrate": "sequelize db:migrate",
    "test:api": "cross-env TEST_MODE=0 LOG_LEVEL=error babel-node --extensions .js,.mjs node_modules/.bin/_mocha --exit --timeout 60000 --reporter spec --ui qunit server/tests/api/*",
    "test": "npm run lint && npm run test:create.db && npm run test:api",
    "test:create.db": "node --experimental-modules server/tests/createDB.mjs",
    "coinpayments:webhooks": "node --es-module-specifier-resolution=node server/tests/use-case/webhook/coinpayments-webhook.mjs",
    "landing-page-wrap-status:as:start": "export REACT_APP_LANDING_PAGE=wrap-status REACT_APP_API_BASE_URL=http://localhost:3004/ PORT=3004 REACT_APP_IS_WRAP_STATUS_PAGE=true && npm run client:start",
    "landing-page-wrap-status:as:build": "export REACT_APP_LANDING_PAGE=wrap-status REACT_APP_IS_WRAP_STATUS_PAGE=true && npm run client:build:no-snap",
    "landing-page-fio-id:as:start": "export REACT_APP_LANDING_PAGE=fio-id REACT_APP_API_BASE_URL=http://localhost:3003/ PORT=3003 && npm run client:start",
    "landing-page-fio-id:as:build": "export REACT_APP_LANDING_PAGE=fio-id && npm run client:build:no-snap",
    "landing-page-fio-wallet-snap:as:start": "export REACT_APP_API_BASE_URL=http://localhost:3002/ PORT=3002 REACT_APP_LANDING_PAGE=fio-wallet-snap && npm run client:start",
    "landing-page-fio-wallet-snap:as:build": "export REACT_APP_LANDING_PAGE=fio-wallet-snap && npm run client:build:no-snap",
    "migration:create": "npx sequelize migration:create --name",
    "prepare": "husky install",
    "analyze": "source-map-explorer 'client/build/static/js/*.js'"
  },
